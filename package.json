{
  "name": "toy",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:3000": "cross-env NODE_PORT=3000 NODE_ENV=development nest start",
    "start:3001": "cross-env NODE_PORT=3001 NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_PORT=3000 NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^1.0.0",
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.3",
    "@nestjs/graphql": "^10.0.8",
    "@nestjs/platform-express": "^8.4.3",
    "@nestjs/platform-socket.io": "^8.4.3",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.4.3",
    "@types/node": "^17.0.23",
    "apollo-server-core": "^3.6.7",
    "apollo-server-express": "^3.6.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.0",
    "graphql": "^16.3.0",
    "ioredis": "^5.0.3",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.6.2",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "socket.io": "^4.4.1",
    "socket.io-redis": "^6.1.1",
    "source-map-support": "^0.5.21",
    "ts-morph": "^14.0.0",
    "typeorm": "0.2.45",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "eslint": "^8.12.0",
    "prettier": "^2.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
